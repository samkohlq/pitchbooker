{
  "name": "pitchbooker",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s --copy-files && rm -rf ./build/tests",
    "clean": "rm -rf build",
    "start": "npm run build && node ./build/bin/www",
    "start:dev": "nodemon --exec babel-node ./src/bin/www",
    "test": "jest --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "regenerator-runtime": "^0.13.3",
    "sequelize": "^5.21.4",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@babel/node": "^7.8.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
